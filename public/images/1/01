        <mxCell id="B4NIz8C4_tN_S_q5Rbla-53" value="&lt;font style=&quot;font-size: 14px;&quot;&gt;Thực hiện A/B testing,&lt;/font&gt;&lt;div&gt;&lt;font style=&quot;font-size: 14px;&quot;&gt;phân khúc người dùng&lt;/font&gt;&lt;/div&gt;" style="edgeLabel;html=1;align=center;verticalAlign=middle;resizable=0;points=[];" vertex="1" connectable="0" parent="B4NIz8C4_tN_S_q5Rbla-50">
          <mxGeometry x="-0.1159" y="-1" relative="1" as="geometry">
            <mxPoint y="14" as="offset" />
